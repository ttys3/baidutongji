{
	"name": "phpbb/baidutongji",
	"type": "phpbb-extension",
	"description": "An extension that allows administrators to easily add their Baidu Tongji tracking code to their phpBB forum.",
	"homepage": "https://www.phpbb.com",
	"version": "1.0.5-dev",
	"keywords": ["phpbb", "extension", "baidu", "analytics", "tongji"],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "荒野無燈",
			"email": "__@ttys3.net",
			"homepage": "https:/ttys3.net",
			"role": "Extensions Dev"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phing/phing": "2.4.*"
	},
	"extra": {
		"display-name": "Baidu Tongji",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0"
		},
		"version-check": {
			"host": "www.phpbb.com",
			"directory": "/customise/db/extension/baidutongji",
			"filename": "version_check"
		}
	}
}
